pipeline {
	agent any

	stages {
		stage('Build'){
			steps{
				echo 'Building'
				}
			}
		
		stage('Deploying'){
			steps{
				echo 'Deploying'
				}
			}

		stage('Moving to Nexus'){
			steps{
				echo 'Moving to Nexus'
				}
			}
		
		stage('Process completed successfully'){
			steps{
				echo 'Completed successfully'
				}
			}
		}
	}
